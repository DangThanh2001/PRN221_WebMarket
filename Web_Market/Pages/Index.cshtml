<section class="latest-product-area pt-130 pb-110">
    <div class="container">
        <div class="row">
            <div class="mx-auto col-xl-6 col-lg-7 col-md-10">
                <div class="text-center section-title mb-60">
                    <h1>Every Products</h1>
                    <p>Looking for the best deals on your favorite products? Look no further than our website market! With a wide range of products from top brands, we have everything you need to make your shopping experience easy and enjoyable. Our user-friendly website makes it simple to find what you're looking for, and with competitive prices and fast shipping, you'll never have to go anywhere else. So why wait? Visit our website market today and start shopping!</p>
                </div>
            </div>
        </div>
        <div class="row">
            <style>
                .overflow-ellip {
                    width: 100% !important; /* Set the width of the container */
                    overflow: hidden; /* Hide any content that overflows the container */
                    white-space: nowrap; /* Prevent line breaks within the text */
                    text-overflow: ellipsis; /* Display an ellipsis at the end of the truncated text */
                }

                .fixed-img {
                    width: 100% !important;
                    height: 500px;
                }
            </style>
            @if (Model.listProduct.Count == 0)
            {
                <div class="col-xl-12 col-lg-12 col-md-12 text-center">
                    <span class="fs-1">sorry, not found any product about this :'(</span>
                </div>
            }
            @foreach (var o in Model.listProduct)
            {
                <div class="col-xl-4 col-lg-6 col-md-6">
                    <div class="single-product">
                        <div class="product-img">
                            <a asp-page="product_detail" asp-route-productid="@o.ProductId">
                                @{
                                    var img = o.Image.Split(';');
                                }
                                <img class="fixed-img" src="@img[0]" alt="">
                            </a>
                            <div class="product-action">
                                <a href="javascript:void(0)"><i class="lni lni-heart"></i></a>
                                <a href="javascript:void(0)" class="share"><i class="lni lni-share"></i></a>
                            </div>
                        </div>
                        <div class="product-content">
                            <h3 class="name"><a class="overflow-ellip" title="@o.ProductName" asp-page="product_detail" asp-route-productid="@o.ProductId">@o.ProductName</a></h3>
                            <span class="update">Last Update: 5 hours ago</span><br>
                            <span class="update">Quantity left: <span class="fw-bold">@o.Quantity</span></span>
                            <ul class="address">
                                <li>
                                    <a href="javascript:void(0)"><i class="lni lni-calendar"></i> @o.ImportDay.ToString("dd/MMM/yyyy")</a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)"><i class="lni lni-map-marker"></i> Canada</a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)"><i class="lni lni-user"></i> Stifen Jon</a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)"><i class="lni lni-package"></i> Used</a>
                                </li>
                            </ul>
                            <div class="product-bottom">
                                <h3 class="price">$@o.Price</h3>
                                <h3 class="">
                                    <a href="#">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-cart-plus" viewBox="0 0 16 16">
                                            <path d="M9 5.5a.5.5 0 0 0-1 0V7H6.5a.5.5 0 0 0 0 1H8v1.5a.5.5 0 0 0 1 0V8h1.5a.5.5 0 0 0 0-1H9V5.5z" />
                                            <path d="M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1H.5zm3.915 10L3.102 4h10.796l-1.313 7h-8.17zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0z" />
                                        </svg>
                                    </a>
                                    </h3>
                            </div>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
    <div>
        <nav aria-label="Page navigation example">
            <ul class="pagination justify-content-center">
                @for (var i = 0; i < Model.maxPage; i++)
                {
                    <li class="page-item">
                        <a asp-page="/Index" asp-route-currentpage="@i" asp-route-keyword="@ViewBag.keyword"
                       asp-route-category="@ViewBag.category" asp-route-company="@ViewBag.company"
                       class="page-link @(Model.curPage == i ? "text-white active-2" : "text-dark")">@(i + 1)</a>
                    </li>
                }
                <style>
                    .active-2 {
                        background-color: #FF6B6B !important;
                    }
                </style>
            </ul>
        </nav>
    </div>
</section>


