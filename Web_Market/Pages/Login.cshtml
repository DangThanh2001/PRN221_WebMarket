@page
@model Web_Market.Pages.Login.LoginModel
@{
    ViewData["Title"] = "Login";
}

<!--[if IE]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
    <![endif]-->

<div class="preloader">
    <div class="loader">
        <div class="ytp-spinner">
            <div class="ytp-spinner-container">
                <div class="ytp-spinner-rotator">
                    <div class="ytp-spinner-left">
                        <div class="ytp-spinner-circle"></div>
                    </div>
                    <div class="ytp-spinner-right">
                        <div class="ytp-spinner-circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


    <section class="banner-area bg_cover">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="banner-content">
                        <h1 class="text-white">Login Page</h1>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a asp-page="index">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Login Page</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>


<section class="login-area pt-70 pb-70">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-10">
                <div class="login-wrapper box-style">
                    <h3 class="mb-20">Login</h3>
                    <form asp-page-handler="login" method="post">
                        <div class="text-danger" asp-validation-summary="ModelOnly"></div>
                        <div class="row">
                            <div class="col-12">
                                <div class="single-form mb-20">
                                    <label asp-for="account.Email" class="mb-2">Email Or Username</label>
                                    <input type="text" id="email" name="email" placeholder="Email Or Username" class="py-2 px-3 mb-0 rounded border">
                                </div>
                                <span class="text-danger" asp-validation-for="account.Email"></span>
                            </div>
                            <div class="col-12">
                                <div class="single-form mb-20">
                                    <label asp-for="account.Password" class="mb-2">Password</label>
                                    <input type="password" id="password" name="password" placeholder="Password" class="py-2 px-3 mb-0 rounded border">
                                </div>
                                <span class="text-danger" asp-validation-for="account.Password"></span>
                            </div>
                            <div class="col-12">
                                <div class="single-form mb-30">
                                    <button class="main-btn btn-hover py-2 px-5 rounded">Login</button>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="single-form mb-20">
                                    <p>Forget Password? <a href="javascript:void(0)">Reset Now</a></p>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="single-form mb-30">
                                    <p>Don't have an Account? <a asp-page="/Sign_up">Sign Up</a></p>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="single-form text-center mb-30">
                                    <h6>Or</h6>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="single-form">
                                    <p class="mb-20">Login with </p>
                                    <div class="row">
                                        <div class="col-6">
                                            <a href="javascript:void(0)" class="main-btn btn-hover py-2 px-4">
                                                <i class="lni lni-facebook-filled"></i>
                                                Facebook
                                            </a>
                                        </div>
                                        <div class="col-6">
                                            <a href="javascript:void(0)" class="main-btn btn-hover py-2 px-4">
                                                <i class="lni lni-google"></i>
                                                Google
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>


<section class="subscribe-area">
    <div class="container">
        <div class="subscribe-wrapper bg_cover">
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="subscribe-content section-title">
                        <h1 class="text-white">Subscribe Our Newsletter</h1>
                        <p class="text-white">Buy and sell everything from used cars to mobile phones and computer or search for property.</p>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="subscribe-form-wrapper">
                        <form action="#" class="subscribe-form">
                            <input type="email" name="sub-email" placeholder="Enter your Email">
                            <button><i class="lni lni-telegram-original"></i></button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<section class="client-logo-area pt-140 pb-110">
    <div class="container">
        <div class="row">
            <div class="mx-auto col-xl-6 col-lg-7 col-md-10">
                <div class="text-center section-title mb-60">
                    <h1>Our Trusted Company</h1>
                    <p>
                        Lorem ipsum dolor sit amet, consetetur sadipscing elitr sed diam nonumy eirmod tempor invidunt ut labore et
                        dolore magna.
                    </p>
                </div>
            </div>
        </div>
        <div class="client-logo-wrapper">
            <div class="row">
                <div class="col-lg-3 col-sm-6">
                    <div class="single-logo">
                        <img src="~/images/client-logo/uideck-logo.svg" alt="">
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="single-logo">
                        <img src="~/images/client-logo/pagebulb-logo.svg" alt="">
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="single-logo">
                        <img src="~/images/client-logo/lineicons-logo.svg" alt="">
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="single-logo">
                        <img src="~/images/client-logo/graygrids-logo.svg" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>




<a href="#" class="back-to-top btn-hover"><i class="lni lni-chevron-up"></i></a>

